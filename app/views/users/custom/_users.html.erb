<div class="all_users">
<h3> All Users</h3>
  <% @users.each do |user| %>
  <div class="user-list-index">
    <% if user.profile_image.attached? %>
      <%=link_to (image_tag user.profile_image, class:"profile_image"), user_feed_path(user_id: user.id)%>
    <% end %>
    <%=link_to "#{user.full_name}", user_feed_path(user_id: user.id)%>
    </div>  
  <% end %>
</div>